<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <string name="app_description">AcDisplay это новый способ управления уведомлениями в Android. Он даст вам знать о новых уведомлениях через прекрасный минималистичный интерфейс, одновременно позволяя легко управлять ими. Если вы хотите узнать что происходит - просто достаньте устройство из кармана.</string>

    <string name="xposed_description">Помогает AcDisplay глубже интегрироваться в систему.</string>

    <!-- App widget -->
    <string name="widget_toggle_enabled">Включен</string>
    <string name="widget_toggle_disabled">Отключен</string>

    <!-- Test notification -->
    <string name="test_action">Проверить работу</string>
    <string name="test_notification_message">Пример того, как выглядит уведомление.</string>
    <string name="test_notification_message_large">Смахните уведомление вниз для его удаления.\nПроведите в любом месте для разблокировки.</string>

    <!-- Initial notification -->
    <string name="init_notification_text">Просто отклоните это уведомление.</string>

    <!-- Remote actions -->
    <string name="remote_enable_acdisplay">AcDisplay включен.</string>
    <string name="remote_disable_acdisplay">AcDisplay отключен.</string>

    <!-- Foreground service that may contain both Keyguard and Active mode. -->
    <string name="service_bath">Сервисы AcDisplay</string>
    <string name="service_bath_keyguard">Экран блокировки</string>
    <string name="service_bath_active_mode">Активный режим</string>

    <!-- Example: Mon, 7 April -->
    <string name="status_date_format">EEE, d MMMM</string>

    <!--
        Device admin / notification access
         -->

    <string name="access_not_granted">Необходимые разрешения:</string>
    <string name="access_notifications_grant_btn">Уведомления</string>
    <!-- Shows as a toast, when clicking a button failed. -->
    <string name="access_notifications_grant_manually">Перейдите в Настройки > Безопасность > Доступ к уведомлениям и включите AcDisplay.</string>
    <string name="access_device_admin_description">Это необходимо для того, что бы управлять собственной задержкой выключения экрана.</string>
    <string name="access_device_admin_grant_btn">Выключить экран</string>
    <!-- Shows as a toast, when clicking a button failed. -->
    <string name="access_device_admin_grant_manually">Перейдите в Настройки > Безопасность > Администраторы устройства и включите AcDisplay</string>
    <string name="access_device_admin_removed_successful">AcDisplay был удален из списка администраторов устройства.</string>
    <string name="app_auto_disabled">AcDisplay был отключен из-за удаления прав администратора.</string>

    <!-- Pressing button goes to previous track. -->
    <string name="media_prev_description">Кнопка перехода к предыдущему треку</string>
    <!-- Pressing button goes to next track. -->
    <string name="media_next_description">Кнопка перехода к следующему треку</string>
    <!-- Pressing button pauses playback -->
    <string name="media_pause_description">Кнопка паузы</string>
    <!-- Pressing button pauses playback -->
    <string name="media_play_description">Кнопка воспроизведения</string>
    <!-- Pressing button pauses playback -->
    <string name="media_stop_description">Кнопка выключения</string>

    <!-- Content description of the battery level icon for accessibility (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_battery_level">Battery <xliff:g id="number" example="100">%d</xliff:g> percent.</string>

</resources>
